{
  "name": "gatsby-typescript-boilerplate",
  "description": "Gatsby Typescript boilerplate",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "browserslist": [
    "defaults",
    "ie >=9"
