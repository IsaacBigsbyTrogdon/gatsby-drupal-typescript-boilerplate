    "react-async": "^10.0.0",
    "react-dom": "^16.6.1",
    "react-helmet": "^5.2.0",
    "react-hook-form": "^4.10.2",
    "react-loop": "^0.1.0",
    "react-responsive": "^8.0.3",
    "react-tracked": "^1.1.1",
    "reactn": "^2.2.6",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6"
  },
  "devDependencies": {
    "@rfgamaral/eslint-config-typescript-unified": "^1.1.6",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.7",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
